<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
      minHeight="-Infinity"
>
    <children>
        <HBox>
            <children>
                <TextField text="input package name to delete" HBox.hgrow="ALWAYS"/>
                <Button mnemonicParsing="false" text="Uninstall"/>
            </children>
        </HBox>
        <CheckBox mnemonicParsing="false" text="keep data when uninstall"/>
        <Region prefHeight="24.0" VBox.vgrow="SOMETIMES"/>
        <Button mnemonicParsing="false" text="Don't know package name? get or drag apk here to identify"/>
    </children>
</VBox>
