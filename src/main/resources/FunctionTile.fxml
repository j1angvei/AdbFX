<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" contentDisplay="TOP" text="Function Name" type="Button" prefWidth="128"
         style="-fx-background-color: transparent"
         prefHeight="128">
    <graphic>
        <ImageView fx:id="imgIcon" fitHeight="72.0" fitWidth="72.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@img/function.png"/>
            </image>
        </ImageView>
    </graphic>
</fx:root>
